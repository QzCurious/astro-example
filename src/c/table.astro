---
interface Props {
  data: {
    name: string;
    age: number;
    city: string;
  }[];
}

const { data } = Astro.props;
---

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Age</th>
      <th>City</th>
    </tr>
  </thead>
  <tbody>
    {
      data.map((item) => (
        <tr>
          <td>{item.name}</td>
          <td>{item.age}</td>
          <td>{item.city}</td>
        </tr>
      ))
    }
  </tbody>
</table>
